{
  "name": "@reactory/client-core-plugin",
  "version": "1.0.0",
  "description": "Reactory Client Core Plugin.",
  "main": "src/index.js",
  "author": "W Weber",
  "license": "Apache-2.0",
  "homepage": "https://bitbucket.org/reactory/reactory-core-client-plugin#readme",
  "scripts": {
    "build": "npx babel src --out-file lib/reactory.core.js",
    "start": "NODE_PATH=./src npx nodemon --exec npx babel-node ./src/index.js",
    "rollup": "rollup --config rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run release",
    "release": "npm run build:dev && npm run build:prod",
    "build:dev": "NODE_ENV=development rollup --config rollup.config.js",
    "build:prod": "NODE_ENV=production rollup --config rollup.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@reactory/reactory-core": "file:../artifacts/reactory-reactory-core-1.0.0.tgz",
    "core-js": "^3.1.4",
    "styled-jsx": ">= 3.x.x"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/reactory/reactory-core-client-plugin.git"
  },
  "keywords": [
    "reactory",
    "core",
    "client",
    "plugin"
  ],
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
